{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Name": {
      "type": "string",
      "maxLength": 25,
      "minLength": 3
    },
    "banner": {
      "type": "media",
      "multiple": true,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "description": {
      "type": "richtext",
      "minLength": 5,
      "maxLength": 300,
      "required": true
    },
    "Price": {
      "type": "integer",
      "required": true,
      "default": 0
    }
  }
}
